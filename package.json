        "@vitejs/plugin-react": "^4.0.4",
        "csv-parse": "^5.5.0",
        "entities": "^4.5.0",
        "eslint": "^8.49.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "firebase-admin": "^11.11.1",
        "node-fetch": "^3.3.2",
        "rollup-plugin-visualizer": "^5.9.2",
        "uuid": "^9.0.1",
        "vite": "^4.4.9"
