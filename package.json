{
  "name": "lpu-belt-explorer",
  "version": "0.0.1",
  "description": "An interactive web app for exploring the Lockpickers United belt ranking system.",
  "main": "index.js",
  "repository": "git@github.com:NiXXeD/lpu-belt-explorer.git",
  "author": "Andy Christianson <nix@nixxed.com>",
  "license": "Unlicense",
  "homepage": "https://lpubelts.com",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production vite build",
    "build-dev": "NODE_ENV=production yarn lint && yarn versionFile && vite build --mode dev",
    "build-dev-import": "NODE_ENV=production yarn versionFile && yarn run import && vite build --mode dev && yarn copyFiles",
    "preview": "vite preview",
    "lint": "eslint \"./src/**\" \"./scripts/**\"",
    "import": "node ./scripts/import.js",
    "import-dan": "node ./scripts/importDan.js",
    "refreshRaflPreview": "node ./scripts/refreshRaflPreview.js",
    "updateRaflForm": "node ./scripts/updateRaflForm.js",
    "migrate-recorded": "node ./scripts/migrateRecorded.js",
    "migrate-black-belts": "node ./scripts/migrateBlackBelts.js",
    "process-changelog": "node ./scripts/processChangelog.js",
    "exportCsv": "node scripts/exportCsv.js",
    "exportMd": "node ./scripts/exportMarkdown.js",
    "exportHtml": "node ./scripts/exportHtml.js",
    "generateSitemap": "node ./scripts/generateSitemap.js",
    "generateSitemap-xml": "node ./scripts/generateSitemap-xml.js",
    "versionFile": "node ./scripts/version.js",
    "min-version": "node ./scripts/minVersion.js",
    "copyData": "node ./scripts/copyData.js",
    "copyFiles": "node ./scripts/copyFiles.js",
    "ci-pr": "yarn lint && yarn build",
    "ci-build": "yarn versionFile && yarn generateSitemap && yarn generateSitemap-xml && yarn run build && yarn exportMd && yarn exportCsv && yarn exportHtml && yarn copyFiles",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "e2e": "clear; playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "codegen": "npx playwright codegen http://localhost:3000/",
    "allure": "allure generate allure-results -o allure-report --clean; allure open allure-report"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.15.4",
    "@mui/styles": "^6.4.11",
    "@mui/x-date-pickers": "^7.10.0",
    "@nivo/bar": "^0.99.0",
    "@nivo/chord": "^0.96.0",
    "@nivo/core": "^0.96.0",
    "@nivo/line": "^0.96.0",
    "@nivo/pie": "^0.96.0",
    "@nivo/sankey": "^0.96.0",
    "@pdf-lib/fontkit": "^1.1.1",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.13.2",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.3",
    "fast-levenshtein": "^3.0.0",
    "firebase": "^11.7.1",
    "fuzzysort": "^3.1.0",
    "heic2any": "^0.0.4",
    "joi": "^17.10.1",
    "motion": "^12.23.24",
    "notistack": "^3.0.2",
    "obscenity": "^0.4.5",
    "pdf-lib": "^1.17.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-full-screen": "^1.1.1",
    "react-hotkeys-hook": "^4.4.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.21.0",
    "react-simple-star-rating": "^5.1.7",
    "react-swipeable": "^7.0.1",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.10",
    "rehype-external-links": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "remove-accents": "^0.5.0",
    "usehooks-ts": "^2.9.4",
    "validator": "^13.15.20"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.21",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "3.2.4",
    "@wdio/allure-reporter": "^9.20.0",
    "allure-playwright": "^3.4.1",
    "csv-parse": "^5.5.0",
    "entities": "^7.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "firebase-admin": "^11.11.1",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "msw": "^2.6.6",
    "node-fetch": "^3.3.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "uuid": "^9.0.1",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  }
}
